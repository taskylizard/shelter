{
  "name": "Shelter MV3 Inj",
  "version": "1.3",
  "homepage_url": "https://github.com/uwu/shelter",
  "description": "Injects shelter on discord.com/app - for MV3 browsers",
  "manifest_version": 3,
  "permissions": ["storage", "declarativeNetRequestWithHostAccess", "declarativeNetRequestFeedback"],
  "host_permissions": ["*://discord.com/*", "*://ptb.discord.com/*", "*://canary.discord.com/*"],
  "content_scripts": [
    {
      "matches": ["https://discord.com/*", "https://ptb.discord.com/*", "https://canary.discord.com/*"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],

  "icons": {},

  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "discord_nocsp",
        "enabled": true,
        "path": "dnr_rules.json"
      }
    ]
  }
}
